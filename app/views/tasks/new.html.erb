<% provide(:title, 'Create task') %>
<h1>Create task</h1>
<div class="row">
  <div class="span6 offset3">
    <%= form_for(@task) do |f| %>
 <%= render 'shared/error_messages', object: f.object %>
      <%= f.label :task ,"task name" %>
      <%= f.text_field :name %> 

      <%= f.label :value ,"credit value" %>
      <%= f.text_field :value %>
  <%= f.submit "Create new task", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>